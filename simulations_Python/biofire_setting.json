{
    "algorithm": {
        "general": {
            "title": "Algorithm to generate and simulate random species communities in a competition-colonization space implicit model with a fire-vegetation feedback",
            "generation": false,
            "save_files": true,
            "simulation": true,
            "figure": true
        },
        "generation": {
            "distribution": "med",
            "n_species": 10,
            "n_communities": 3
        },
        "simulation": {
            "runtime": 100,
            "repetition": 1,
            "delta_day": 1,
            "epsilon": 0.001,
            "minfirerettime": 1,
            "firevf": 100000,
            "n_coms": [],
            "n_init": []
        }
    },
    "output": {
      "data": {
        "path": "path_to_data_archive/{}{}/",
        "file_str": "{}files_{}pft-{}y-{}run-{}noise.dat",
        "file_info": "{}-n{}-{}-info.dat",
        "file_bave": "{}-n{}-{}-bave-{}y.dat",
        "file_fave": "{}-n{}-{}-fave-{}y.dat"
      },
      "figure": {
        "path": "path_to_figure/{}{}/",
        "fig_name": "{}-n{}-{}-{}y-init{}.png"
      }
    },
    "log": {
      "filename": "log_file.txt",
      "folder": "log_folder"
    }
  }
  